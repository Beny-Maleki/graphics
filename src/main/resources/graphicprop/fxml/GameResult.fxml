<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="700.0" prefWidth="1000.0" styleClass="pane" stylesheets="@../stylesheets/GameResultPage.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.controller.GameResultView">
   <children>
      <Pane layoutX="145.0" layoutY="103.0" prefHeight="425.0" prefWidth="710.0" style="-fx-background-color: blue;">
         <children>
            <Label fx:id="title" layoutX="245.0" layoutY="19.0" text="Winner" textFill="WHITE">
               <font>
                  <Font size="48.0" />
               </font>
            </Label>
            <ImageView fitHeight="331.0" fitWidth="310.0" layoutX="400.0" layoutY="101.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/kaiba1.png" />
               </image>
            </ImageView>
            <Label fx:id="winner" alignment="CENTER" layoutX="52.0" layoutY="143.0" prefHeight="143.0" prefWidth="303.0" textFill="WHITE">
               <font>
                  <Font size="48.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Button fx:id="back" layoutX="474.0" layoutY="554.0" mnemonicParsing="false" onMouseClicked="#run" onMouseEntered="#hoverAnimation" text="Back" />
      <Button fx:id="nextRound" layoutX="457.0" layoutY="594.0" mnemonicParsing="false" onMouseClicked="#run" onMouseEntered="#hoverAnimation" text="Next Round" />
   </children>
</AnchorPane>
